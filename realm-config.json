{
  "realm": "${REALM_NAME}",
  "enabled": true,
  "users": [
    {
      "username": "${REALM_ADMIN_USER}",
      "enabled": true,
      "email": "${REALM_ADMIN_USER}@gmail.com",
      "firstName": "${REALM_ADMIN_USER}_FIRST_NAME",
      "lastName": "${REALM_ADMIN_USER}_LAST_NAME",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "${REALM_ADMIN_PASSWORD}"
        }
      ],
      "realmRoles": ["admin"]
    },
    {
      "username": "${REALM_NORMAL_USER}",
      "enabled": true,
      "email": "${REALM_NORMAL_USER}@gmail.com",
      "firstName": "${REALM_NORMAL_USER}_FIRST_NAME",
      "lastName": "${REALM_NORMAL_USER}_LAST_NAME",
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "${REALM_NORMAL_PASSWORD}"
        }
      ],
      "realmRoles": ["user"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin"
      },
      {
        "name": "user"
      }
    ]
  },
  "clients": [
    {
      "clientId": "${REALM_CLIENT_ID}",
      "publicClient": false,
      "redirectUris": ["*"],
      "clientAuthenticatorType": "client-secret",
      "secret": "${REALM_CLIENT_SECRET}",
      "directAccessGrantsEnabled": true
    }
  ]
}
