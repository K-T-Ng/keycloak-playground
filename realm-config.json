{
  "realm": "${REALM_NAME}",
  "enabled": true,
  "users": [
    {
      "username": "${REALM_ADMIN_USER}",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "${REALM_ADMIN_PASSWORD}"
        }
      ],
      "realmRoles": ["admin"]
    },
    {
      "username": "${REALM_NORMAL_USER}",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "${REALM_NORMAL_PASSWORD}"
        }
      ],
      "realmRoles": ["user"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin"
      },
      {
        "name": "user"
      }
    ]
  },
  "clients": [
    {
      "clientId": "myclient",
      "publicClient": true,
      "redirectUris": ["*"],
      "directAccessGrantsEnabled": true
    }
  ]
}
